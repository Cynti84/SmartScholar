    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.0",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "@types/validator": "^13.15.3",
    "nodemon": "^3.1.10",
